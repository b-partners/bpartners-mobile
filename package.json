{
  "name": "bpartners-mobile",
  "version": "1.22.19",
  "private": true,
  "scripts": {
    "start": "NODE_OPTIONS=\"--max_old_space_size=8192\" react-native start",
    "ios": "react-native run-ios --simulator=\"iPhone 15 Pro Max\"",
    "android": "NODE_OPTIONS=\"--max_old_space_size=8192\" react-native run-android",
    "build:ios:debug:e2e": "detox build --configuration ios.sim.debug",
    "build:ios:release:e2e": "detox build --configuration ios.sim.release",
    "build:android:debug:e2e": "detox build --configuration android.emu.debug",
    "build:android:release:e2e": "detox build --configuration android.emu.release",
    "build:android:device:debug:e2e": "detox build --configuration android.att.debug",
    "build:android:device:release:e2e": "detox build --configuration android.att.release",
    "test:ios:debug:e2e": "detox test --configuration ios.sim.debug",
    "test:ios:release:e2e": "detox test --configuration ios.sim.release",
    "test:android:debug:e2e": "detox test --configuration android.emu.debug",
    "test:android:release:e2e": "detox test --configuration android.emu.release",
    "test:android:device:debug:e2e": "detox test --configuration android.att.debug",
    "test:android:device:release:e2e": "detox test --configuration android.att.release",
    "test:maestro": ".maestro test .maestro/index.yml",
    "test:android": "maestro test maestro/index.yml",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm run format:json && npm run format:md && npm run format:ts && npm run format:yml",
    "format:js": "prettier --write \"app/**/*.js\" \"src/**/*.js\"",
    "format:json": "prettier --write \"app/**/*.json\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:ts": "prettier --write \"app/**/*.ts{,x}\"",
    "format:yml": "prettier --write \"./**/*.{yml,yaml}\"",
    "lint": "eslint index.js app storybook --fix --ext .js,.ts,.tsx && npm run format",
    "check": "eslint --fix-dry-run . && prettier --check \"app/**/*.{js,ts,tsx}\" \"./**/*.{yml,yaml}\"",
    "patch": "patch-package",
    "storybook": "start-storybook -p 9001 -c ./storybook",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "node ./bin/postInstall",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean": "react-native-clean-project",
    "clean-all": "npx react-native clean-project-auto",
    "prepare": "husky install",
    "eas-build-pre-install": "cp .env.template .env"
  },
