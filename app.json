{
  "name": "BpartnersMobile",
  "displayName": "BpartnersMobile",
  "expo": {
    "name": "BpartnersMobile",
    "scheme": "app.bpartners",
    "slug": "bpartners",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-all.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
